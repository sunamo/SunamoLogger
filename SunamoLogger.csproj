<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.2.1.2</Version>
    <Description>Support for several logging system based on their output</Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SunamoBts" Version="24.2.1.1" />
    <PackageReference Include="SunamoClipboard" Version="24.2.1.1" />
    <PackageReference Include="SunamoCollectionsIndexesWithNull" Version="24.2.1.1" />
    <PackageReference Include="SunamoI18N" Version="24.1.23.1" />
    <PackageReference Include="SunamoReflection" Version="24.2.1.1" />
    <PackageReference Include="SunamoStringFormat" Version="24.2.1.1" />
    <PackageReference Include="SunamoStringJoinPairs" Version="24.2.1.1" />
    <PackageReference Include="SunamoThisApp" Version="24.2.1.1" />
    <PackageReference Include="SunamoInterfaces" Version="24.2.1.2" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Interfaces\" />
  </ItemGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
</Project>